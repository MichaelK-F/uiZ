/// @description uiz_easybutton_setcolor(instanceid,color[c_white],textcolor[c_black])
/// @param instanceid
/// @param color[c_white]
/// @param textcolor[c_black]
function uiz_easybutton_setcolor(argument0, argument1, argument2) {
	gml_pragma("forceinline");
	argument0.color=argument1;
	argument0.textcolor=argument2;



}
