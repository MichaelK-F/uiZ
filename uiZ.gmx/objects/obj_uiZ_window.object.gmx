        //right bottom
        isresizing_y = uiz_bottom;
        isresizing_x = uiz_right;
        uiz_set_cursor(cr_size_nwse);
    } else {
        //check left bottom
        if (uiz_getmouse_x() &lt; rx + 20 and uiz_getmouse_y() &gt; rly - 20) then {
            //left bottom
