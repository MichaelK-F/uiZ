        uiz_updater_FixViews_area_selfmarked(rx,ry+startOffset+(lastSelectionPos-startPos)*fontHeight,srlx,ry+startOffset+(lastSelectionPos-startPos+1)*fontHeight-1);    
    }
}

if draggingItem!=-1 and !mouse_check_button(mb_left) then{
        var srlx = rlx;
