            <string>uiz_frameset_colanchor_CalculateOverFrameBar();
kmouseover = uiz_mouse_isonobject_leftcheck(id);

if kmouseover==uiz_mousereleased {
    if snaptomouse = 1 then {
        stopsnap = 0
        snaptomouse = 0
        uiz_mouse_unfreeze();
        uiz_updater_unstep();
