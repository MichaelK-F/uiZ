    var cx = frameat[snaptobar].rx + margin + max(uiz_getposx(frameat[snaptobar].minsizeinframeset, frameat[snaptobar].minsizeinframesetvaltype), du)

    if uiz_getmouse_x() &lt; 0 or uiz_getmouse_y() &lt; 0 or uiz_getmouse_x() &gt; global.screenpxwidth or uiz_getmouse_y() &gt; global.screenpxheight then {
        stopsnap = 1
    }

    if stopsnap = 1 then {
        mousediff = 0
    } else {
        mousediff = lastmouse - uiz_getmouse_x()
    }
    lastmouse = uiz_getmouse_x()

    if isizetype[snaptobar] = xtra and isizetype[snaptobar + 1] = xtra then {
        //convert the first to a factor
        isizetype[snaptobar] = fc
        //set appropriate size
        isize[snaptobar] = frameat[snaptobar].width / width
    }
    stopsnap = 0
    switch (isizetype[snaptobar]) {
        case px:
            isize[snaptobar] -= mousediff
            if uiz_getmouse_x() &lt; cx then {
                isize[snaptobar] = nx stopsnap = 1
            }
            if uiz_getmouse_x() &gt; cwidth then {
                isize[snaptobar] = nwidth stopsnap = 1
            }
            break;
        case fc:
            isize[snaptobar] = min(1 - 1 / width, max(1 / width, ((isize[snaptobar] * width) - mousediff) / width))
            if uiz_getmouse_x() &lt; cx then {
                isize[snaptobar] = nx / width stopsnap = 1
            }
            if uiz_getmouse_x() &gt; cwidth then {
                isize[snaptobar] = nwidth / width stopsnap = 1
            }
            break;
        case dp:
            isize[snaptobar] -= mousediff / uiz_dp
            if uiz_getmouse_x() &lt; cx then {
                isize[snaptobar] = nx / uiz_dp stopsnap = 1
            }
            if uiz_getmouse_x() &gt; cwidth then {
                isize[snaptobar] = nwidth / uiz_dp stopsnap = 1
            }
            break;
    }
    switch (isizetype[snaptobar + 1]) {
        case px:
            isize[snaptobar + 1] += mousediff
            if uiz_getmouse_x() &lt; cx then {
                isize[snaptobar] = nwidth stopsnap = 1
            }
            if uiz_getmouse_x() &gt; cwidth then {
                isize[snaptobar] = nx stopsnap = 1
            }
            break;
        case fc:
            isize[snaptobar + 1] = min(1 - 1 / width, max(1 / width, ((isize[snaptobar + 1] * width) + mousediff) / width))
            if uiz_getmouse_x() &lt; cx then {
                isize[snaptobar] = nwidth / width stopsnap = 1
            }
            if uiz_getmouse_x() &gt; cwidth then {
