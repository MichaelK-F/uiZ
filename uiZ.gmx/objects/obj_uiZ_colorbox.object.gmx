        global.colorscreenobject = id
        w = uiz_window_create(4, dp, 5, dp)
        w.x = uiz_getmouse_x()
        w.y = uiz_getmouse_y()
        w.button_maximize = 0;
        w.button_minimize = 0;
        w.button_cross = 1;
        w.contain = 3
        w.windowtext = "colorpicker"
        //w.resizable = false;
        uiz_window_setminimumsize(w, 2, dp, 3, dp)
        uiz_fixgeneralpos(w)
        f = w;
        fs = uiz_frameset_create();
        uiz_setParent(fs, f)
        div1 = uiz_frameset_divide_vertical(fs, 0.7, fc, 1, xtra)
